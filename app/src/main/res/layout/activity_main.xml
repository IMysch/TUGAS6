<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="584dp"
        android:layout_height="986dp"
        app:srcCompat="@drawable/gambar2"
        tools:layout_editor_absoluteX="-93dp"
        tools:layout_editor_absoluteY="-1dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="487dp"
        android:layout_height="626dp"
        app:srcCompat="@drawable/gambar3"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="-148dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="261dp"
        android:layout_height="348dp"
        app:srcCompat="@drawable/gambar1"
        tools:layout_editor_absoluteX="-41dp"
        tools:layout_editor_absoluteY="-18dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="163dp"
        android:layout_height="88dp"
        android:text="Ikhsan Mutaqin"
        tools:layout_editor_absoluteX="124dp"
        tools:layout_editor_absoluteY="616dp" />

</androidx.constraintlayout.widget.ConstraintLayout>

